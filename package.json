{
  "name": "vue-input-keyboard",
  "version": "1.0.180712",
  "description": "A keyboard of input with validator, for: numbers, telephone, IDs...",
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "unpkg": "dist/index.min.js",
  "browser": {
    "./sfc": "src/inputkeyboard.vue"
  },
  "scripts": {
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/index.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/index.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/index.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wanyaxing/vue-input-keyboard.git"
  },
  "keywords": [
    "vue",
    "input",
    "keyboard"
  ],
  "author": "wyx@wanyaxing.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wanyaxing/vue-input-keyboard/issues"
  },
  "homepage": "https://github.com/wanyaxing/vue-input-keyboard#readme",
  "devDependencies": {
    "rollup": "^0.57.1",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-vue": "^3.0.0",
    "rollup-plugin-uglify": "^4.0.0",
    "uglify-es": "^3.3.9",
    "vue": "^2.5.16",
    "vue-template-compiler": "^2.5.16"
  },
  "dependencies": {
  }
}
